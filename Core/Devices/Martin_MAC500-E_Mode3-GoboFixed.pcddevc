  procedure DoReset;
  begin
    if MessageDlg('Shall the MovingHead be RESET?',mtConfirmation,[mbYes,mbNo],0)=mrYes then
    begin
	    set_channel('color1',12,12,0);
	    set_channel('prisma',80,80,0);
	    set_channel('gobo1',0,0,0);

	    set_channel('shutter',210,210,0);
			ShowMessage('Klick "OK", if RESET has been initiated...');
	    set_channel('shutter',0,0,0);

	    set_channel('color1',0,0,0);
	    set_channel('prisma',0,0,0);
	    set_channel('gobo1',0,0,0);
    end;
  end;
