 <code>
  unit Mac500;

  interface

  procedure InitDevice(Device: String);
  procedure FormShow;
  procedure FormRefresh(channel:Integer);
  procedure FormClose;
  procedure ColorBoxChange;
  procedure PositionXYChange(Top, Left: Integer);
  procedure ChangeDimmer;
  procedure ChangeGobo;
  procedure ChangeGoboRot;
  procedure FocusChange;
  procedure PrismaChange;
  procedure LampOff;
  procedure LampOn;
  procedure DoReset;
  procedure ShutterChange;
  procedure ColorBoxChange2;
  procedure IrisChange;
  procedure PantiltSpeedChange;

  implementation

  var
    dontrefresh:boolean;

  procedure InitDevice(Device: String);
  begin
    // diese Funktion wird für alle Geräte diesen Typs beim ersten Start des PC_DIMMERs ausgeführt.
  end;
  procedure FormShow;
  begin
  	dontrefresh:=false;
  end;
  procedure FormRefresh(channel:Integer);
  var
    wert:integer;
    wert2:integer;
  begin
  case channel of
    	0: // Shutter
    	begin
    		wert:=get_channel('shutter');
    		case wert of
    			0..19: shutterbox.itemindex:=0;
    			20..49: shutterbox.itemindex:=1;
    			50..72:
					begin
						shutterbox.itemindex:=2;
		    		if not dontrefresh then
							shutterslider.position:=wert-50;
					end;
    			73..79: shutterbox.itemindex:=1;
    			80..99:
					begin
						shutterbox.itemindex:=3;
		    		if not dontrefresh then
							shutterslider.position:=wert-80;
					end;
    			100..119:
					begin
						shutterbox.itemindex:=4;
		    		if not dontrefresh then
							shutterslider.position:=wert-100;
					end;
    			120..127: shutterbox.itemindex:=1;
    			128..147: shutterbox.itemindex:=5;
    			148..167: shutterbox.itemindex:=6;
    			168..187: shutterbox.itemindex:=7;
    			188..190: shutterbox.itemindex:=1;
    			191..193: shutterbox.itemindex:=8;
    			194..196: shutterbox.itemindex:=9;
    			197..199: shutterbox.itemindex:=10;
    			200..202: shutterbox.itemindex:=11;
    			203..207: shutterbox.itemindex:=1;
    			218..227: shutterbox.itemindex:=1;
    			238..247: shutterbox.itemindex:=1;
    		end;
    	end;
    	1: // Dimmer
    	begin
    		wert:=get_channel('dimmer');
    		if not dontrefresh then
    		begin
    			dimmerslider.position:=wert;
    		end;
    	end;
    	2: // Color1
    	begin
    		wert:=get_channel('color1');
			if not dontrefresh then
    		case wert of
    			0..15: 
			begin
				colorbox.itemindex:=0;
	    			Color1Slide.position:=wert*2;
			end;
    			16..31: 
			begin
				colorbox.itemindex:=1;
	    			Color1Slide.position:=(wert-16)*2;
			end;
    			32..47: 
			begin
				colorbox.itemindex:=2;
	    			Color1Slide.position:=(wert-32)*2;
			end;
    			48..63: 
			begin
				colorbox.itemindex:=3;
	    			Color1Slide.position:=(wert-48)*2;
			end;
    			64..79:	
			begin
				colorbox.itemindex:=4;
	    			Color1Slide.position:=(wert-64)*2;
			end;
    			80..95:
			begin
				colorbox.itemindex:=5;
	    			Color1Slide.position:=(wert-80)*2;
			end;
    			96..101:
			begin
				colorbox.itemindex:=6;
	    			Color1Slide.position:=(wert-96)*2;
			end;
   			112..127:
			begin
				colorbox.itemindex:=7;
	    			Color1Slide.position:=(wert-112)*2;
			end;
    			128..144:
			begin
				colorbox.itemindex:=8;
	    			Color1Slide.position:=(wert-128)*2;
			end;
    			145..148: colorbox.itemindex:=9;
    			149..152: colorbox.itemindex:=10;
    			153..156: colorbox.itemindex:=11;
    			157..160: colorbox.itemindex:=12;
    			161..164: colorbox.itemindex:=13;
    			165..168: colorbox.itemindex:=14;
    			169..172: colorbox.itemindex:=15;
    			173..176: colorbox.itemindex:=16;
    			177..180: colorbox.itemindex:=17;
    			181..184: colorbox.itemindex:=18;
    			185..215:
			begin
				colorbox.itemindex:=19;
	    			Color1Slide.position:=wert-185;
			end;
			216..245:
			begin
				colorbox.itemindex:=20; 
	    			Color1Slide.position:=wert-216;
			end;
    		end;
    	end;
    	3: // Color2
    	begin
    		wert:=get_channel('color2');
			if not dontrefresh then
    		case wert of
    			0..15: 
			begin
				colorbox2.itemindex:=0;
	    			Color2Slide.position:=wert*2;
			end;
    			16..31: 
			begin
				colorbox2.itemindex:=1;
	    			Color2Slide.position:=(wert-16)*2;
			end;
    			32..47: 
			begin
				colorbox2.itemindex:=2;
	    			Color2Slide.position:=(wert-32)*2;
			end;
    			48..63: 
			begin
				colorbox2.itemindex:=3;
	    			Color2Slide.position:=(wert-48)*2;
			end;
    			64..79:	
			begin
				colorbox2.itemindex:=4;
	    			Color2Slide.position:=(wert-64)*2;
			end;
    			80..95:
			begin
				colorbox2.itemindex:=5;
	    			Color2Slide.position:=(wert-80)*2;
			end;
    			96..101:
			begin
				colorbox2.itemindex:=6;
	    			Color2Slide.position:=(wert-96)*2;
			end;
   			112..127:
			begin
				colorbox2.itemindex:=7;
	    			Color2Slide.position:=(wert-112)*2;
			end;
    			128..144:
			begin
				colorbox2.itemindex:=8;
	    			Color2Slide.position:=(wert-128)*2;
			end;
    			145..148: colorbox2.itemindex:=9;
    			149..152: colorbox2.itemindex:=10;
    			153..156: colorbox2.itemindex:=11;
    			157..160: colorbox2.itemindex:=12;
    			161..164: colorbox2.itemindex:=13;
    			165..168: colorbox2.itemindex:=14;
    			169..172: colorbox2.itemindex:=15;
    			173..176: colorbox2.itemindex:=16;
    			177..180: colorbox2.itemindex:=17;
    			181..184: colorbox2.itemindex:=18;
			185..215:
			begin
				colorbox2.itemindex:=19;
	    			Color2Slide.position:=wert-185;
			end;
			216..245:
			begin
				colorbox2.itemindex:=20; 
	    			Color2Slide.position:=wert-216;
			end;
      			246..248: colorbox2.itemindex:=21;
    			249..251: colorbox2.itemindex:=22;
    			252..255: colorbox2.itemindex:=23;
    			
    		end;
    	end;
	4..5: // Gobo1Rotation
    	begin
   		wert:=get_channel('gobo1rot');
    		if not dontrefresh then
    		 begin
		  case wert of
			0..55: 
			begin
				GoboIndexSelect.itemindex:=0;
				goborotslider.position:=0;
			end;
